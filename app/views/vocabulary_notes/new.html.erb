<h1>Create New Vocabulary Note</h1>

<%= form_with(model: @vocabulary_note, url: vocabulary_notes_path) do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :description %>
  <%= form.text_area :description %>

  <%= form.submit 'Create Vocabulary Note' %>
<% end %>

<%= link_to 'Back to Vocabulary Notes', vocabulary_notes_path %>
